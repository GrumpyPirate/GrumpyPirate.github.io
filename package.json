{
  "name": "ecobbold-portfolio-2017",
  "version": "3.0.0",
  "engines": {
    "node": "^10.0.0",
    "npm": "^6.0.0"
  },
  "description": "Personal portfolio website of Edward Cobbold, a Frontend Web Developer",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "start": "npm run build && NODE_ENV=production node server.js",
    "analyze-bundle": "NODE_ENV=production webpack --config webpack.config.analyze.js",
    "test": "NODE_ENV=test jest --watch",
    "lint:js": "eslint \"./client/**/*.{js,jsx}\" -c ./.eslintrc.json",
    "lint:styles": "stylelint \"./client/**/*.scss\" --quiet --config ./.stylelintrc",
    "lint:all": "npm run lint:js && npm run lint:styles"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "contentful": "^7.4.1",
    "core-js": "^3.0.0",
    "devicon": "2.0.0",
    "express": "^4.16.4",
    "feather-icons": "^4.19.0",
    "lodash-es": "^4.17.11",
    "morgan": "1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.4",
    "react-markdown": "^4.0.6",
    "react-redux": "^6.0.1",
    "react-responsive-carousel": "^3.1.47",
    "react-router-dom": "4.3.1",
    "react-transition-group": "^2.6.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "sanitize.css": "^8.0.0",
    "smoothscroll-polyfill": "^0.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "cssnano-preset-advanced": "^4.0.7",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.7.1",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
