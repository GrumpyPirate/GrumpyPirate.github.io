{
  "name": "ecobbold-portfolio-2017",
  "version": "1.0.0",
  "engines": {
    "node": "8.x",
    "npm": "5.0.x"
  },
  "description": "Personal portfolio website of Edward Cobbold, a Frontend Web Developer",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "bootstrap": "^4.0.0-alpha.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "contentful": "^4.5.0",
    "css-loader": "0.28.4",
    "devicon": "^2.0.0",
    "dotenv": "4.0.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "fbemitter": "^2.1.1",
    "feather-icons": "^2.4.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "image-webpack-loader": "^3.3.1",
    "jest": "20.0.4",
    "jquery": "3",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-normalize-scss": "^3.0.0",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "popper.js": "^1.12.3",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "react": "^15",
    "react-dev-utils": "^3.0.2",
    "react-dom": "15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-responsive-carousel": "^3.1.24",
    "react-router-dom": "^4.1.2",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "svg-sprite-loader": "^3.0.8",
    "svgo-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "react-test-renderer": "15"
  },
  "scripts": {
    "dev": "node scripts/start.js",
    "build": "node scripts/build.js",
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node ./server.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/(?!src).*/.*"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "es2015"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "jest",
      "jasmine"
    ],
    "extends": ["standard", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "fetch": true
    },
    "env": {
      "jest/globals": true,
      "jasmine": true
    }
  }
}
