    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./client/**/*.{js,jsx}": [
      "prettier-eslint --write --eslint-config-path ./.eslintrc.json",
      "git add"
    ],
    "./client/**/*.scss": [
      "stylelint --fix --quiet --config ./.stylelintrc",
      "git add"
    ]
  }
}
